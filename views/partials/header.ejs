                <a class="nav-link" href="/user/<%= currentUser.id %>/edit">Edit Profile</a>
              </li>
              <li class="nav-item active">
                <a class="nav-link" href="/user/<%= currentUser.id %>/manage">Manage Account</a>
              </li>
            </ul>
          </li>
  
